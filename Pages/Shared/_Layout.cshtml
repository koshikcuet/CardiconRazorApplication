    <div class="container-fluid">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <br />
    <br />
    <br />
    <footer class="border-top footer bg-black text-white text-center">
        <div class="container" style="padding:5px">
            <div class="col-12" style="font-size:1rem; white-space:normal; line-height:20px;">
                &copy; Cardicon 2023 - International Conference on Cardiovascular Diseases.
            </div>
            
